extends ../layout-index
block content
    body.lang-cn
        .publish-body
          .banner-other.banner-other-wankebi
            .banner-txt
              h3 基于区块链的精准扶贫应用管理平台
              p 不忘初心 砥砺前行
            a.banner-link-block(href='#')
          .publish-tab-box
            .publish-tab
              .publish-tab-list
                a(href='/admin/project/new')
                  i.icon.iconfont.icon-download
                  | 项目申请
              .publish-tab-list
                a(href='#')
                  i.icon.iconfont.icon-intro
                  | 项目审批
              .publish-tab-list
                a(href='#')
                  i.icon.iconfont.icon-ssh
                  | 资金拨付
              .publish-tab-list
                a(href='/admin/project/list?page=0')
                  i.icon.iconfont.icon-home
                  | 项目查询
          .publish-mainer
            #app
              div(data-reactid='.0')
                p.publish-date(data-reactid='.0.0')
                  span(data-reactid='.0.0.0') 数据统计时间：
                  span.publish-date-span(data-reactid='.0.0.1') 2018年02月20日
                .publish-section.publish-section-date(data-reactid='.0.1')
                  .publish-box-num.clear(data-reactid='.0.1.0')
